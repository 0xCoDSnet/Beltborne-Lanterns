{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": ["${mod_authors}"],
  "license": "${mod_license}",
  "contact": {
    "sources": "${mod_sources}",
    "homepage": "${mod_homepage}"
  },
  "icon": "assets/beltborne_lanterns/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.oxcodsnet.beltborne_lanterns.fabric.BLFabric"
    ],
    "fabric-datagen": [
      "net.oxcodsnet.beltborne_lanterns.fabric.datagen.BLDataGenerator"
    ],
    "client": [
      "net.oxcodsnet.beltborne_lanterns.fabric.client.BLFabricClient"
    ],
    "modmenu": [
      "net.oxcodsnet.beltborne_lanterns.fabric.client.BLFabricModMenu"
    ],
    "lambdynlights.initializer": [
      "net.oxcodsnet.beltborne_lanterns.fabric.compat.LambDynLightsInitializer"
    ]
  },
  "mixins": [
    "beltborne_lanterns.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": "~${minecraft_version}",
    "java": ">=17",
    "fabric-api": "*",
    "cloth-config2": ">=${cloth_config_version}"
  },
  "suggests": {
    "modmenu": "${modmenu_version}"
  }
}
